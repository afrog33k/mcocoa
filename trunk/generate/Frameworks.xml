			<Type name = "NSAppleScript" model = "main"/>
			<Type name = "NSArchiver" model = "main"/>
			<Type name = "NSBundle" model = "main"/>
			<Type name = "NSCalendar" model = "main"/>
			<Type name = "NSCoder" model = "main"/>
